<% require_relative '../youtubeSearch' %>



<head>
<title>VideoShare</title>
</head>
<body>
 <form >
  Search Video <br>
  <input type = "text" name = "SearchVideo" ><br>
  
  <input type = "submit" value = "Search">
</form> 
<%	if !@videos.empty? %>
	<% @videos.each do |video| %>
    	<iframe width="420" height="315"
		src=<%=video%>>
			</iframe>
    <% end %>
<% end %>
</body>
